doctype html
| {% load staticfiles %}
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    // The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags
    meta(name='google-signin-client_id', content='803317838435-gg18me1ju2c4759bs24kq1oi313dr3br.apps.googleusercontent.com')
    title eywa
    // Bootstrap
    link(href="{% static 'ai/css/bootstrap.min.css' %}", rel='stylesheet')
    // Custom css
    link(rel='stylesheet', href="{% static 'ai/css/chatstyle.css' %}")
    link(rel='stylesheet', href="{% static 'ai/css/animate.css' %}")
    // Roboto font from google
    link(href='https://fonts.googleapis.com/css?family=Roboto:100', rel='stylesheet', type='text/css')
    link(href='https://fonts.googleapis.com/css?family=Pacifico', rel='stylesheet', type='text/css')
    link(href='https://fonts.googleapis.com/css?family=Comfortaa', rel='stylesheet', type='text/css')
    link(href='https://fonts.googleapis.com/css?family=Quicksand:700', rel='stylesheet', type='text/css')
    link(href='https://fonts.googleapis.com/css?family=Open+Sans', rel='stylesheet', type='text/css')
    // HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src='https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js')
      script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
    script(src='https://apis.google.com/js/platform.js', async='', defer='')
  body
    | {% block content %}
    //
      All the content goes in here, and all the importing and stuff goes on the top!
      Nothing is to be done in other html files
    | {% endblock %}
    // jQuery (necessary for Bootstrap's JavaScript plugins)
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js')
    // Include all compiled plugins (below), or include individual files as needed
    // <script src="js/bootstrap.min.js"></script>
    script(src="{% static 'ai/js/jquery.js' %}")
    // Bootstrap Core JavaScript
    script(src="{% static 'ai/js/bootstrap.min.js' %}")
    // Google Maps API Key - Use your own API key to enable the map feature. More information on the Google Maps API can be found at https://developers.google.com/maps/
